{
  "mcpServers": {
    "hpkv-memory-server-http": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://memory-mcp.hpkv.io/sse",
        "--transport",
        "http-first",
        "--debug"
      ]
    },
    "local-memory-fallback": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    }
  }
} 